    <p class="text-sm font-medium text-gray-500">Identificator</p>
    <%= position_group.identificator %>
  </div>
  <div class="mb-4">
    <p class="text-sm font-medium text-gray-500">Name</p>
    <%= position_group.name %>
  </div>
  <div class="mb-4">
    <p class="text-sm font-medium text-gray-500">Description</p>
    <%= position_group.description %>
  </div>
  <div class="mb-4">
    <p class="text-sm font-medium text-gray-500">Value</p>
    <%= position_group.value %>
  </div>
